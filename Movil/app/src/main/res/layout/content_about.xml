<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_about"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.daniel.proyecto3_bases.AboutActivity">

    <TextView
        android:text="Jairo Méndez Martínez"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/textViewJairo"
        android:textAppearance="@android:style/TextAppearance.Medium" />

    <TextView
        android:text="Daniel Moya Sánchez"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewDaniel"
        android:textAppearance="@android:style/TextAppearance.Medium"
        android:layout_below="@+id/textViewJairo"
        android:layout_alignLeft="@+id/textViewJason"
        android:layout_alignStart="@+id/textViewJason"
        android:layout_marginTop="42dp" />

    <TextView
        android:text="Jason Leitón Jiménez"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewJason"
        android:textAppearance="@android:style/TextAppearance.Medium"
        android:layout_above="@+id/textViewJairo"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="37dp" />

    <TextView
        android:text="Francisco Alvarado Ferllini"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewFran"
        android:textAppearance="@android:style/TextAppearance.Medium"
        android:layout_marginBottom="40dp"
        android:layout_above="@+id/textViewJason"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Propierty of:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_above="@+id/textViewFran"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp"
        android:layout_marginBottom="52dp" />
</RelativeLayout>



